ed with 'to'*/}\n            <Link to={`/product/${product._id}`}>\n                <Card.Img src={product.image} variant='top' />\n            </Link>\n            <Card.Body>\n                <Link to={`/product/${product._id}`}>\n                    <Card.Title as='div'>\n                        <strong>\n                            {product.name}\n                        </strong>\n                    </Card.Title>\n                </Link>\n                <Card.Text as='div'>\n                    {/* taking in two props value and text (talking with Rating component) */}\n                    <Rating value={product.rating} text={`${product.numReviews} reviews`}>\n                        {product.rating} from {product.numReviews} reviews}\n                    </Rating>\n                </Card.Text>\n                <Card.Text as='h3'>\n                    ${product.price}\n\n                </Card.Text>\n                <Card.Text>\n                    <Button\n                        onClick={addToCartHandler}\n                        className='btn btn-block'\n                        type='button'\n                        // disabled={product.countInStock === 0 }\n                    >\n                        ADD TO CART\n                    </Button>\n                </Card.Text>\n            </Card.Body>\n        </Card>\n    )\n}\n\nexport default Product","/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/AlertDismissible.js",["253"],"import React, {useState} from 'react';\nimport {Button, Alert, Col} from 'react-bootstrap';\n\nconst AlertDismissible = (props) => {\n    const [show, setShow] = useState(false)\n\n\n    return (\n        <>\n                <p>\n                    <h5 className='mt-2'>{props.title}</h5>\n                    <Button onClick={() => setShow(true)} variant='danger' >{props.icon}</Button>\n\n                </p>\n\n\n            {show &&\n\n                <Alert onClose={() => setShow(false)} dismissible >\n     