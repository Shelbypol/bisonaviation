ents/Footer.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/RegisterScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/ProductScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/ProductsDisplayScreen.js",["269","270"],"import React, { useEffect } from 'react'\nimport { Link} from \"react-router-dom\";\nimport { useDispatch, useSelector } from 'react-redux'\nimport { Row, Col} from 'react-bootstrap'\nimport Product from '../components/Product'\nimport Message from '../components/Message'\nimport Loader from '../components/Loader'\nimport { listProducts } from '../actions/productActions'\nimport Paginate from '../components/Paginate'\nimport Meta from \"../components/Meta\";\nimport ProductCarousel from \"../components/ProductCarousel\";\n\nconst ProductsDisplayScreen = ({ match, history }) => {\n\n    const keyword = match.params.keyword;\n\n    const pageNumber = match.params.pageNumber || 1;\n\n    const dispatch = useDispatch();\n\n    const productList = useSelector(state => state.productList);\n    const { loading, error, products, pages, page } = productList;\n\n    useEffect(() => {\n        dispatch(listProducts(keyword, pageNumber));\n\n    },[dispatch, keyword, pageNumber]);\n\n\n    return (\n        <>\n            <Meta title='Bison | Products' />\n\n            {/*{!keyword ? <ProductCarousel /> : <Link to='/' className='btn btn-light'>Back</Link>}*/}\n\n            <h1>Latest Products</h1>\n            {loading ?\n                ( <Loader/> )\n                : error ?\n                    ( <Message variant='danger'>{error}</Message> )\n                    : (\n                        <>\n                            <Paginate pages={pages} page={page} keyword={keyword ? keyword : ''} />\n                            <Row>\n                                {products.map(product => (\n                                    <Col key={product._id} sm={12} md={6} lg={4}>\n                                        <Product product={product} history={history} productId={product._id}/>\n                                    </Col>\n                                ))}\n                            </Row>\n                            <Paginate pages={pages} page={page} keyword={keyword ? keyword : ''} />\n                        </>\n                    )\n            }\n        </>\n    )\n};\n\nexport default ProductsDisplayScreen;","/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/LoginScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/CartScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/ShippingScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/ProfileScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/PaymentScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/UserListScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/OrderScreen.js",["271"],"import React, { useState ,useEffect} from \"react\";\nimport axios from 'axios';\nimport { PayPalButton } from 'react-paypal-button-v2'\nimport {Link} from \"react-router-dom\";\nimport { Row, Col, ListGroup, Image, Card, Button} from \"react-bootstrap\";\nimport {useDispatch, useSelector} from \"react-redux\";\nimport Message from \"../components/Message\";\nimport Loader from \"../components/Loader\";\nimport {getOrderDetails, payOrder, deliverOrder } from \"../actions/orderActions\";\nimport { ORDER_PAY_RESET, ORDER_DELIVER_RESET } from \"../constants/orderConstants\";\nimport {CART_RESET, CART_SAVE_SHIPPING_ADDRESS} from \"../constants/cartConstants\";\n\nconst OrderScreen = ({ match, history }) => {\n    const orderId = match.params.id;\n\n    // software dev kit ( sdk ) that paypal gives us\n    const [sdkReady, setSdkReady] = useState(false);\n\n    const dispatch = useDispatch();\n\n    const orderDetails = useSelector((state) => state.orderDetails);\n    const {order, loading, error} = orderDetails;\n\n    const orderPay = useSelector((state) => state.orderPay);\n    const {loading: loadingPay , success: successPay} = orderPay;\n\n    const orderDeliver = useSelector((state) => state.orderDeliver);\n    const {loading: loadingDeliver , success: successDeliver} = orderDeliver;\n\n    const userLogin = useSelector((state) => state.userLogin);\n    const { userInfo } = userLogin;\n\n    const addDecimals = (num) => {\n        return (Math.round(num * 100) / 100).toFixed(2)\n    };\n\n    if (!loading) {\n        order.itemsPrice = addDecimals(\n            order.orderItems.reduce((acc, item) => acc + item.price * item.qty, 0)\n        );\n    }\n\n    useEffect(() => {\n        if(!userInfo) {\n            history.push('/login')\n        }\n\n        const addPayPalScript = async () => {\n            const { data: clientId } = await axios.get('/api/config/paypal');\n            const script = document.createElement('script');\n            script.type = 'text/javascript';\n            script.src  = `https://www.paypal.com/sdk/js?client-id=${clientId}`;\n            script.async = true;\n            script.onload = () => {\n                setSdkReady(true)\n            };\n            document.body.appendChild(script)\n        };\n\n        if(!order || order._id !== orderId || successPay || successDeliver) {\n            // if(!order || successPay || successDeliver) {\n            dispatch({ type: ORDER_PAY_RESET });\n            dispatch({ type: ORDER_DELIVER_RESET });\n            dispatch(getOrderDetails(orderId))\n        } else if(!order.isPaid) {\n            if(!window.paypal) {\n                addPayPalScript();\n            } else {\n                setSdkReady(true);\n            }\n        }\n    }, [dispatch, order, orderId, successPay, successDeliver, history, userInfo]);\n\n    // takes in payment result from paypal\n    const successPaymentHandler = (paymentResult) => {\n        console.log(paymentResult);\n        dispatch(payOrder(orderId, paymentResult));\n\n        // Clear Cart on success payment\n        // console.log(successPay);\n        // if(successPay){\n        dispatch({ type: CART_RESET });\n        // {history.push(`/thankyou/${paymentResult}/${orderId}`)}\n        // }\n    };\n\n    const deliverHandler = () => {\n        dispatch(deliverOrder(order))\n    };\n\n    return loading ? <Loader/>\n        : error\n            ? <Message variant='danger'>{error}</Message>\n            : <>\n                <h1 className='inline'>Order # </h1>\n                <h4 className='inline'> {order._id}</h4>\n                <Row>\n                    <Col me={8}>\n                        <ListGroup>\n                            <ListGroup.Item variant='flush'>\n                                <h2>Shipping</h2>\n                                <p>\n                                    <strong>Name: </strong>\n                                    {order.user.name}\n                                </p>\n                                <p>\n                                    <strong>Email: </strong>\n                                    <a href={`mailto:${order.user.email}`}>{order.user.email}</a>\n                                </p>\n                                <div>\n                                    <strong>Address: </strong>\n                                    <br/>\n                                    <p style={{marginLeft: '1em'}}>\n                                        {order.shippingAddress.address},<br/>\n                                        {order.shippingAddress.city},<br/>\n                                        {order.shippingAddress.postalCode}, {order.shippingAddress.country}\n                                    </p>\n                                </div>\n                                {order.isDelivered ? (\n                                    <Message variant='success'>Delivered on {order.deliveredAt}</Message>\n                                ) : (\n                                    <Message variant='danger'>Not Delivered</Message>\n                                )}\n                            </ListGroup.Item>\n\n                            {/* PAYMENT METHOD */}\n                            <ListGroup.Item>\n                                <h2>Payment Method</h2>\n                                <p>\n                                    <strong>Method: </strong>\n                                    {order.paymentMethod}\n                                </p>\n                                {order.isPaid ? (\n                                    <>\n                                        <Message variant='success' >Paid on {order.paidAt}</Message>\n                                        {history.push(`/thankyou/${orderId}`)}\n                                    </>\n                                ) : (\n                                    <Message variant='danger'>Not paid</Message>\n                                )}\n                            </ListGroup.Item>\n\n                            <ListGroup.Item>\n                                <h2>Order Items</h2>\n                                {order.orderItems.length === 0\n                                    ? <Message>Order is empty</Message>\n                                    : (\n                                        <ListGroup variant='flush'>\n                                            {order.orderItems.map((item, index) => (\n                                                <ListGroup.Item key={index}>\n                                                    <Row>\n                                                        <Col md={1}>\n                                                            <Image src={item.image} alt={item.name} fluid rounded/>\n                                                        </Col>\n                                                        <Col>\n                                                            <Link to={`/product/${item.product}`}>\n                                                                {item.name}\n                                                            </Link>\n                                                        </Col>\n                                                        <Col md={4}>\n                                                            {item.qty} x $ {item.price} = $ {item.qty * item.price}\n                                                        </Col>\n                                                    </Row>\n                                                </ListGroup.Item>\n                                            ))}\n                                        </ListGroup>\n                                    )}\n                            </ListGroup.Item>\n                        </ListGroup>\n                    </Col>\n\n                    {/* ORDER SUMMARY */}\n                    <Col md={4}>\n                        <Card>\n                            <ListGroup variant='flush'>\n                                <ListGroup.Item>\n                                    <h2>Order Summary</h2>\n                                </ListGroup.Item>\n                                <ListGroup.Item>\n                                    <Row>\n                                        <Col>Items</Col>\n                                        <Col>$ {order.itemsPrice}</Col>\n                                    </Row>\n                                </ListGroup.Item>\n\n                                {/* SHIPPING */}\n                                <ListGroup.Item>\n                                    <Row>\n                                        <Col>Shipping</Col>\n                                        <Col>$ {order.shippingPrice}</Col>\n                                    </Row>\n                                </ListGroup.Item>\n\n                                {/* TAX */}\n                                <ListGroup.Item>\n                                    <Row>\n                                        <Col>Tax</Col>\n                                        <Col>$ {order.taxPrice}</Col>\n                                    </Row>\n                                </ListGroup.Item>\n\n                                {/* TOTAL */}\n                                <ListGroup.Item>\n                                    <Row>\n                                        <Col>Total</Col>\n                                        <Col>$ {order.totalPrice}</Col>\n                                    </Row>\n                                </ListGroup.Item>\n                                {!order.isPaid && (\n                                    <ListGroup.Item>\n                                        {loadingPay && <Loader/>}\n                                        {!sdkReady ? <Loader/> : (\n                                            <PayPalButton\n                                                amount={order.totalPrice}\n                                                onSuccess={successPaymentHandler}\n                                            />\n                                        )}\n                                    </ListGroup.Item>\n                                )}\n                                {loadingDeliver && <Loader />}\n                                {userInfo && userInfo.isAdmin && order.isPaid && !order.isDelivered && (\n                                    <ListGroup.Item>\n                                        <Button type='button' className='btn btn-block' onClick={deliverHandler} >\n                                            Mark as delivered\n                                        </Button>\n                                    </ListGroup.Item>\n                                )}\n                            </ListGroup>\n                        </Card>\n                    </Col>\n                </Row>\n            </>\n};\n\nexport default OrderScreen","/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/ProductListScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/UserEditScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/AvionicsScreen.js",["272","273"],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/PlaceOrderScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/HomeScreen.js",["274","275","276","277"],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/ProductEditScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/OrderListScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/AircraftPartsScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/ThankYouScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/AogResponseScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/MaintenanceScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/AcquisitionScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/SystemSalesScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/EoIrMaintenanceScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/TeamScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/EoIrPartsScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/SearchBox.js",["278"],"import React, {useState} from 'react'\nimport {Form, Button, Nav} from 'react-bootstrap'\n\nconst SearchBox = ({history, navClasses ,searchClasses, formClasses, buttonClasses, buttonStyle, searchSize}) => {\n    const [keyword, setKeyword] = useState('');\n\n    const submitHandler = (e) => {\n        e.preventDefault();\n        if (keyword.trim()) {\n            history.push(`/search/${keyword}`);\n        } else {\n            history.push('/')\n        }\n        setKeyword('')\n    };\n\n    return (\n            <Form onSubmit={submitHandler} inline className={searchClasses}>\n                <Form.Control\n                    type='text'\n                    name='searchBar'\n                    value={keyword}\n                    placeholder='Search for Garmin Products...'\n                    onChange={(e) => setKeyword(e.target.value)}\n                    size={searchSize}\n                    className={formClasses}\n                >\n                </Form.Control>\n                <Button type='submit' size='lg'\n                        style={{buttonStyle}} className={buttonClasses}\n                >\n                    <i style={{color: 'black'}} className=\"fas fa-search\"> </i>\n                </Button>\n            </Form>\n    )\n};\n\nexport default SearchBox","/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/FormContainer.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/Message.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/Loader.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/Rating.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/Meta.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/Product.js",["279","280"],"import React from 'react';\n//import Link for Router\nimport { Link  } from 'react-router-dom'\nimport {Button, Card, ListGroup} from 'react-bootstrap'\nimport Rating from './Rating'\nimport {useSelector} from \"react-redux\";\n\n// access props (product._id and product.image) through the ({ product )} in the function param\nconst Product = ({ product, history, productId }) => {\n\n    //HANDLERS\n    const addToCartHandler = () => {\n        console.log(product._id);\n        history.push(`/cart/${productId}?qty=1`);\n    };\n\n\n    return (\n        <Card className='my-3 p-3 rounded'>\n            {/* Replace 'a' tags with Link from router and href is replaced with 'to'*/}\n            <Link to={`/product/${product._id}`}>\n                <Card.Img src={product.image} variant='top' />\n            </Link>\n            <Card.Body>\n                <Link to={`/product/${product._id}`}>\n                    <Card.Title as='div'>\n                        <strong>\n                            {product.name}\n                        </strong>\n                    </Card.Title>\n                </Link>\n                <Card.Text as='div'>\n                    {/* taking in two props value and text (talking with Rating component) */}\n                    <Rating value={product.rating} text={`${product.numReviews} reviews`}>\n                        {product.rating} from {product.numReviews} reviews}\n                    </Rating>\n                </Card.Text>\n                <Card.Text as='h3'>\n                    ${product.price}\n\n                </Card.Text>\n                <Card.Text>\n                    <Button\n                        onClick={addToCartHandler}\n                        className='btn btn-block'\n                        type='button'\n                        // disabled={product.countInStock === 0 }\n                    >\n                        ADD TO CART\n                    </Button>\n                </Card.Text>\n            </Card.Body>\n        </Card>\n    )\n}\n\nexport default Product","/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/AlertDismissible.js",["281"],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/HomeCarousel.js",["282"],"import React, {useState} from 'react';\nimport {\n    Carousel,\n    CarouselItem,\n    CarouselControl,\n    CarouselIndicators,\n    CarouselCaption\n} from 'reactstrap';\nimport '../style/homeCarousel.css'\n\nimport slide1 from '../images/homeSlider/fullscreen-slider2_2000w_1000h.jpg'\nimport slide2 from '../images/homeSlider/fullscreen-slider3_2000_1000.jpg'\nimport slide3 from '../images/homeSlider/fullscreen-slider5_2000_1000.jpg'\nimport slide4 from '../images/homeSlider/fullscreen-slider4_2000_1000.jpg'\nimport slide5 from '../images/homeSlider/fullscreen-slider1_2000_1000.jpg'\n\nimport overlay from '../images/graphics/fs-slider-caption-bg_1000_450.png';\n\nconst items = [\n    {\n        src: slide1,\n        altText: 'Bison Aviation alt',\n        title: 'BISON AVIATION',\n        text: 'WE WORK HARD ON THE GROUND, SO YOU CAN FLY WITH CONFIDENCE IN THE SKY.'\n    },\n    {\n        src: slide2,\n        altText: 'Slide 2',\n        title: 'Slide 2',\n        text: 'FROM MINOR REPAIRS TO COMPLETE PANEL UPGRADES. ALL PERFORMED IN-HOUSE AT BISON AVIATION.'\n    },\n    {\n        src: slide3,\n        altText: 'Slide 3',\n        title: 'Slide 3',\n        text: 'FROM MINOR REPAIRS TO COMPLETE PANEL UPGRADES. ALL PERFORMED IN-HOUSE AT BISON AVIATION.'\n    },\n    {\n        src: slide4,\n        altText: 'Slide 3',\n        title: 'Slide 3',\n        text: 'FROM MINOR REPAIRS TO COMPLETE PANEL UPGRADES. ALL PERFORMED IN-HOUSE AT BISON AVIATION.'\n    },\n    {\n        src: slide5,\n        altText: 'Slide 3',\n        title: 'Slide 3',\n        text: 'FROM MINOR REPAIRS TO COMPLETE PANEL UPGRADES. ALL PERFORMED IN-HOUSE AT BISON AVIATION.'\n    }\n];\n\nconst HomeCarousel = (props) => {\n    const [activeIndex, setActiveIndex] = useState(0);\n    const [animating, setAnimating] = useState(false);\n\n    const next = () => {\n        if (animating) return;\n        const nextIndex = activeIndex === items.length - 1 ? 0 : activeIndex + 1;\n        setActiveIndex(nextIndex);\n    };\n\n    const previous = () => {\n        if (animating) return;\n        const nextIndex = activeIndex === 0 ? items.length - 1 : activeIndex - 1;\n        setActiveIndex(nextIndex);\n    };\n\n    const goToIndex = (newIndex) => {\n        if (animating) return;\n        setActiveIndex(newIndex);\n    };\n\n    const slides = items.map((item) => {\n        return (\n            <CarouselItem\n                onExiting={() => setAnimating(true)}\n                onExited={() => setAnimating(false)}\n                key={item.src}\n            >\n                <h1 className='carousel-caption imgTitle'>{item.title}</h1>\n                <h5 className='carousel-caption imgText'>{item.text}</h5>\n\n                {/*     SLIDER IMGS     */}\n                <img className=\"img-fluid slider-img col-xs-12\" src={item.src} alt={item.altText}/>\n\n                {/*      OVERLAY IMAGE      */}\n                <img id='overlayImg' className='img-fluid' src={overlay} alt=\"\"/>\n\n            </CarouselItem>\n\n        );\n    });\n\n    return (\n        <>\n            <Carousel\n                activeIndex={activeIndex}\n                next={next}\n                previous={previous}\n            >\n                <CarouselIndicators items={items} activeIndex={activeIndex} onClickHandler={goToIndex}/>\n                {slides}\n                <CarouselControl direction=\"prev\" directionText=\"Previous\" onClickHandler={previous}/>\n                <CarouselControl direction=\"next\" directionText=\"Next\" onClickHandler={next}/>\n            </Carousel>\n\n        </>\n    );\n};\n\nexport default HomeCarousel",["283","284"],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/CheckoutSteps.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/Paginate.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/constants/productConstants.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/constants/cartConstants.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/constants/userConstants.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/constants/orderConstants.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/actions/productActions.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/actions/orderActions.js",[],"/Users/