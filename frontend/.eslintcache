ons.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/constants/productConstants.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/actions/userActions.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/constants/userConstants.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/RegisterScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/HomeScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/Header.js",["198","199"],"import React from 'react'\nimport { Route } from 'react-router-dom'\nimport {Navbar, Nav, Container, NavDropdown} from 'react-bootstrap'\n// whenever you bring something in from the state it's useSelector\n// if you want to call an action it's useDispatch\nimport { useDispatch, useSelector} from 'react-redux'\n// LinkContainer does the same thing as link\nimport { LinkContainer } from 'react-router-bootstrap'\nimport {logout} from '../actions/userActions'\nimport SearchBox from \"./SearchBox\";\nimport {CART_RESET} from \"../constants/cartConstants\";\n\n\nconst Header = () => {\n\n    const dispatch = useDispatch();\n\n    const userLogin = useSelector(state => state.userLogin);\n    const { userInfo } = userLogin;\n\n    const logoutHandler = () => {\n        // window.localStorage.clear();\n        dispatch({ type: CART_RESET});\n        dispatch(logout());\n    };\n\n    return (\n        <header>\n            {/*Add the variant dark or else it will be dark bg with dark text*/}\n            <Navbar bg=\"dark\" variant='dark' expand=\"lg\" collapseOnSelect>\n                <Container>\n                    <LinkContainer to='/'>\n                        <Navbar.Brand>ProShop</Navbar.Brand>\n                    </LinkContainer>\n                    <Navbar.Toggle aria-controls=\"basic-navbar-nav\"/>\n                    <Navbar.Collapse id=\"basic-navbar-nav\">\n\n                        <Route render={({ history}) => <SearchBox history={history} />} />\n\n                        <Nav className=\"ml-auto\">\n                            <LinkContainer to='/cart'>\n                                <Nav.Link>\n                                    <i className='fas fa-shopping-cart'> </i> Cart\n                                </Nav.Link>\n                            </LinkContainer>\n                            {userInfo ? (\n                                <NavDropdown title={userInfo.name} id={'username'}>\n                                    <LinkContainer to='/profile'>\n                                        <NavDropdown.Item>Profile</NavDropdown.Item>\n                                    </LinkContainer>\n\n                                    <NavDropdown.Item onClick={logoutHandler}>Logout</NavDropdown.Item>\n                                </NavDropdown>\n                            ) : <LinkContainer to='/login'>\n                                <Nav.Link><i className='fas fa-user'> </i> Sign in\n                                </Nav.Link>\n                            </LinkContainer>\n                            }\n\n                            {!userInfo && (\n                                <LinkContainer to='/Register'>\n                                    <Nav.Link><i className='fas fa-user'> </i> Register\n                                    </Nav.Link>\n                                </LinkContainer>)\n                            }\n\n                            {userInfo && userInfo.isAdmin && (\n                                <NavDropdown title='Admin' id='adminmenu'>\n                                    <LinkContainer to='/admin/userlist'>\n                                        <NavDropdown.Item>Users</NavDropdown.Item>\n                                    </LinkContainer>\n                                    <LinkContainer to='/admin/productlist'>\n                                        <NavDropdown.Item>Products</NavDropdown.Item>\n                                    </LinkContainer>\n                                    <LinkContainer to='/admin/orderlist'>\n                                        <NavDropdown.Item>Orders</NavDropdown.Item>\n                                    </LinkContainer>\n                                </NavDropdown>\n                            )}\n\n                        </Nav>\n                    </Navbar.Collapse>\n                </Container>\n            </Navbar>\n        </header>\n    )\n};\n\n// export as default means when we import on App.js we dont need to wrap in curly brackets ie: import {Header} is now import Header\nexport default Header","/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/ProductsDisplayScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/HomeCarousel.js",[],{"ruleId":"200","replacedBy":"201"},{"ruleId":"202","replacedBy":"203"},{"ruleId":"204","severity":1,"message":"205","line":11,"column":21,"nodeType":"206","messageId":"207","endLine":11,"endColumn":47},{"ruleId":"200","replacedBy":"208"},{"ruleId":"202","replacedBy":"209"},{"ruleId":"204","severity":1,"message":"210","line":4,"column":23,"nodeType":"206","messageId":"207","endLine":4,"endColumn":32},{"ruleId":"204","severity":1,"message":"211","line":6,"column":9,"nodeType":"206","messageId":"207","endLine":6,"endColumn":20},{"ruleId":"204","severity":1,"message":"212","line":3,"column":47,"nodeType":"206","messageId":"207","endLine":3,"endColumn":52},{"ruleId":"204","severity":1,"message":"213","line":10,"column":8,"nodeType":"206","messageId":"207","endLine":10,"endColumn":15},"no-native-reassign",["214"],"no-negated-in-lhs",["215"],"no-unused-vars","'CART_SAVE_SHIPPING_ADDRESS' is defined but never used.","Identifier","unusedVar",["214"],["215"],"'ListGroup' is defined but never used.","'useSelector' is defined but never used.","'Image' is defined but never used.","'divider' is defined but never used.","no-global-assign","no-unsafe-negation"]