ent global_blood-red global_cursor'>\n                                                {showAd && (\n                                                    <p>x</p>\n                                                )}\n                                            </Button>\n                                        </Col>\n                                            <Col xs={11} className='d-flex justify-content-center px-1'>\n\n                                                {showAd && (\n                                                    <ProductHeroAd products={products}/>\n                                                )}\n                                        </Col>\n\n                                        <Col xs={11} className='px-1 global_white-bg'>\n                                            <ProductsImgDisplay products={products}\n                                                                history={history}\n                                                                match={match}\n                                                                keyword={keyword}\n                                                                pages={pages}\n                                                                page={page}\n                                                                updateCatProp={updateCat}\n                                                                updateManufacturerProp={updateManufacturer}\n                                                                sideBar={sideBar}\n                                            />\n                                        </Col>\n                                    </Row>\n                                </Col>\n                            </Row>\n                        </>\n                    )}\n        </>\n    )\n};\n\nexport default ProductsPage","/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/Product(s)/ProductsImgDisplay.js",[],"/Users/shelby/IdeaProjects/bison-av