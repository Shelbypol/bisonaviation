*  AVIONICS  */}\n                    <LinkContainer\n                        className='d-inline global_whitehovergoldenrodtxt global_goldenrod-border-left-hover '\n                        to='/avionics'>\n                        <Nav.Link>\n                            AVIONICS\n                        </Nav.Link>\n                    </LinkContainer>\n\n                    {/*  MAINTENANCE  */}\n                    <LinkContainer\n                        className='d-inline global_whitehovergoldenrodtxt global_goldenrod-border-left-hover '\n                        to='/maintenance'>\n                        <Nav.Link>\n                            MAINTENANCE\n                        </Nav.Link>\n                    </LinkContainer>\n\n                    {/*  EO/IR FLIR  */}\n                    <LinkContainer\n                        className='d-inline global_whitehovergoldenrodtxt global_goldenrod-border-left-hover '\n                        to='/flir'>\n                        <Nav.Link>\n                            FLIR\n\n                        </Nav.Link>\n                    </LinkContainer>\n\n                    {/*  TEAM  */}\n                    <LinkContainer\n                        className='d-inline global_whitehovergoldenrodtxt global_goldenrod-border-left-hover '\n                        to='/team'>\n                        <Nav.Link>\n                            TEAM\n                        </Nav.Link>\n                    </LinkContainer>\n\n                    {/*  WISHLIST  */}\n                    <LinkContainer\n                        className='d-inline global_whitehovergoldenrodtxt global_goldenrod-border-left-hover '\n                        to='/wishlist'>\n                        <Nav.Link className='' style={{height: '6vh'}}>\n                            <>\n                                        <span className={\n                                            cartItems.length > 0 ?\n                                                