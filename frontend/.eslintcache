port { Row, Col} from 'react-bootstrap'\nimport Product from '../components/Product'\nimport Message from '../components/Message'\nimport Loader from '../components/Loader'\nimport { listProducts } from '../actions/productActions'\nimport Paginate from '../components/Paginate'\nimport Meta from \"../components/Meta\";\nimport ProductCarousel from \"../components/ProductCarousel\";\n\nconst ProductsDisplayScreen = ({ match, history }) => {\n\n    const keyword = match.params.keyword;\n\n    const pageNumber = match.params.pageNumber || 1;\n\n    const dispatch = useDispatch();\n\n    const productList = useSelector(state => state.productList);\n    const { loading, error, products, pages, page } = productList;\n\n    useEffect(() => {\n        dispatch(listProducts(keyword, pageNumber));\n\n    },[dispatch, keyword, pageNumber]);\n\n\n    return (\n        <>\n            <Meta title='Bison | Products' />\n\n            {/*{!keyword ? <ProductCarousel /> : <Link to='/' className='btn btn-light'>Back</Link>}*/}\n\n            <h1>Latest Products</h1>\n            {loading ?\n                ( <Loader/> )\n                : error ?\n                    ( <Message variant='danger'>{error}</Message> )\n                    : (\n                        <>\n                            <Paginate pages={pages} page={page} keyword={keyword ? keyword : ''} />\n                            <Row>\n                                {products.map(product => (\n                                    <Col key={product._id} sm={12} md={6} lg={4}>\n                                        <Product product={product} history={history} productId={product._id}/>\n                                    </Col>\n                                ))}\n                            </Row>\n                            <Paginate pages={pages} page={page} keyword={keyword ? keyword : ''} />\n                        </>\n                    )\n            }\n        </>\n    )\n};\n\nexport default ProductsDisplayScreen;","/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/Header.js",["232"],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/ProductEditScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/EoIrPartsScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/reducers/productReducers.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/screens/TeamScreen.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/reducers/orderReducers.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/reducers/cartReducers.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/reducers/userReducers.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/Message.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/Rating.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/Loader.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/Meta.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/FormContainer.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/SearchBox.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/Paginate.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/HomeCarousel.js",["233"],"import React, {useState} from 'react';\nimport {\n    Carousel,\n    CarouselItem,\n    CarouselControl,\n    CarouselIndicators,\n    CarouselCaption\n} from 'reactstrap';\nimport slide3 from '../images/homeSlider/fullscreen-slider5.jpg'\nimport slide2 from '../images/homeSlider/fullscreen-slider1.jpg'\nimport slide5 from '../images/homeSlider/fullscreen-slider3.jpg'\nimport slide4 from '../images/homeSlider/fullscreen-slider4.jpg'\nimport slide1 from '../images/homeSlider/fullscreen-slider2.jpg'\nimport overlay from '../images/graphics/fs-slider-caption-bg.png';\nimport divider from '../images/graphics/divider1.png'\nimport Header from \"./Header\";\n\nconst items = [\n    {\n        src: slide1,\n        altText: 'Bison Aviation alt',\n        caption: 'BISON AVIATION',\n        text: 'WE WORK HARD ON THE GROUND, SO YOU CAN FLY WITH CONFIDENCE IN THE SKY.'\n    },\n    {\n        src: slide2,\n        altText: 'Slide 2',\n        caption: 'Slide 2',\n        text: 'FROM MINOR REPAIRS TO COMPLETE PANEL UPGRADES. ALL PERFORMED IN-HOUSE AT BISON AVIATION.'\n    },\n    {\n        src: slide3,\n        altText: 'Slide 3',\n        caption: 'Slide 3'\n    },\n    {\n        src: slide4,\n        altText: 'Slide 3',\n        caption: 'Slide 3'\n    },\n    {\n        src: slide5,\n        altText: 'Slide 3',\n        caption: 'Slide 3'\n    }\n];\n\nconst HomeCarousel = (props) => {\n    const [activeIndex, setActiveIndex] = useState(0);\n    const [animating, setAnimating] = useState(false);\n\n    const next = () => {\n        if (animating) return;\n        const nextIndex = activeIndex === items.length - 1 ? 0 : activeIndex + 1;\n        setActiveIndex(nextIndex);\n    };\n\n    const previous = () => {\n        if (animating) return;\n        const nextIndex = activeIndex === 0 ? items.length - 1 : activeIndex - 1;\n        setActiveIndex(nextIndex);\n    };\n\n    const goToIndex = (newIndex) => {\n        if (animating) return;\n        setActiveIndex(newIndex);\n    };\n\n    const slides = items.map((item) => {\n        return (\n            <CarouselItem\n                onExiting={() => setAnimating(true)}\n                onExited={() => setAnimating(false)}\n                key={item.src}\n            >\n                <img className=\"d-block w-100\" src={item.src} alt={item.altText}/>\n                {/*<img className='img-fluid' src={item.src} alt={item.altText}/>*/}\n                {/*      OVERLAY / DIVIDER IMAGE      */}\n                <img id='overlayImg' className=' img-fluid' src={overlay} alt=\"\"/>\n                <img id='dividerImg' className=' img-fluid' src={divider} alt=\"\"/>\n                <CarouselCaption captionText={item.text} captionHeader={item.caption}/>\n            </CarouselItem>\n\n        );\n    });\n\n    return (\n        <>\n            <Carousel\n                activeIndex={activeIndex}\n                next={next}\n                previous={previous}\n                // className='col-xl-10 col-sm-12 m-xl-auto'\n            >\n\n                <CarouselIndicators items={items} activeIndex={activeIndex} onClickHandler={goToIndex}/>\n                {slides}\n                <CarouselControl direction=\"prev\" directionText=\"Previous\" onClickHandler={previous}/>\n                <CarouselControl direction=\"next\" directionText=\"Next\" onClickHandler={next}/>\n            </Carousel>\n\n            {/*      OVERLAY / DIVIDER IMAGE      */}\n            {/*<img id='overlayImg' className=' img-fluid' src={overlay} alt=\"\"/>*/}\n            {/*<img id='dividerImg' className=' img-fluid' src={divider} alt=\"\"/>*/}\n\n        </>\n    );\n};\n\nexport default HomeCarousel","/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/ProductCarousel.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/Product.js",["234","235"],"import React from 'react';\n//import Link for Router\nimport { Link  } from 'react-router-dom'\nimport {Button, Card, ListGroup} from 'react-bootstrap'\nimport Rating from './Rating'\nimport {useSelector} from \"react-redux\";\n\n// access props (product._id and product.image) through the ({ product )} in the function param\nconst Product = ({ product, history, productId }) => {\n\n    //HANDLERS\n    const addToCartHandler = () => {\n        console.log(product._id);\n        history.push(`/cart/${productId}?qty=1`);\n    };\n\n\n    return (\n        <Card className='my-3 p-3 rounded'>\n            {/* Replace 'a' tags with Link from router and href is replaced with 'to'*/}\n            <Link to={`/product/${product._id}`}>\n                