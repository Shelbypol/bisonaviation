         setGoingUp(false);\n            }\n\n            if (prevScrollY.current < currentScrollY && !goingUp) {\n                setGoingUp(true);\n            }\n\n            prevScrollY.current = currentScrollY;\n        };\n\n        window.addEventListener(\"scroll\", handleScroll, {passive: true});\n\n        return () => window.removeEventListener(\"scroll\", handleScroll);\n    }, [goingUp, userInfo, cartItems]);\n\n    return (\n        <>\n            <Row className='global_dots HeaderDesktop_nav-color sticky-top' style={{top: '0'}}>\n\n                <Col xs={3} className=' d-flex justify-content-center align-items-center' style={{height: '12vh'}}>\n                    <LinkContainer to='/'>\n                        <img\n                            src={logo}\n                            className='HeaderDesktop_logo px-1 pt-0'\n                            alt=\"Bison Aviation\"/>\n                    </LinkContainer>\n                </Col>\n\n\n                {/*/ =========== PROFILE / ADMIN BTNS ============*!/*/}\n                <Col xs={9} className='HeaderDesktop_profile-admin-btns-row ml-auto d-flex justify-content-end'>\n\n                    {!userInfo && (\n                        <span className=' d-flex align-items-center justify-content-end'>\n                                        <SignInBtn/>\n                            </span>\n                    )}\n\n                    {userInfo && (\n                        <>\n                            {/*{userInfo.isAdmin ? (*/}\n                            <Link to='#' className='global_goldenrod-border-right'>\n                                <h6\n                                    className=' global_white pr-3'\n                                    style={{fontWeight: 'lighter', paddingTop: '2.5vh'}}>{userInfo.name}</h6>\n                            </Link>\n\n                        </>\n                    )}\n\n                    {userInfo && (\n                        <>\n                            {(userInfo && userInfo.isAdmin) && (\n                                <>\n                                    <LinkContainer to='/admin/userlist' className='d-flex justify-content-center '>\n                                        <Nav.Link className='HeaderDesktop_user-btns'>Users</Nav.Link>\n                                    </LinkContainer>\n\n                                    <LinkContainer to='/admin/productlist'\n                                                   className='d-flex justify-content-center '>\n                                        <Nav.Link className='HeaderDesktop_user-btns'>\n                                            Products\n                                        </Nav.Link>\n                                    </LinkContainer>\n\n                                    {/*<LinkContainer to='/admin/orderlist' className='d-flex justify-content-center mt-1' style={{height: '5vh'}}>*/}\n                                    {/*    <Nav.Link className='SideBar_Name border-right'>*/}\n                                    {/*        Data - Info*/}\n                                    {/*    </Nav.Link>*/}\n                                    {/*</LinkContainer>*/}\n\n                                </>\n                            )}\n\n                            {/*     PROFILE AND LOGOUT BTNS    */}\n                            <Nav style={{zIndex: '15000'}}>\n                                <SignInBtn/>\n                            </Nav>\n                        </>\n                    )}\n                </Col>\n\n\n                <Col xs={7}\n                     className='ml-auto mr-5 pb-0 d-flex justify-content-center HeaderDesktop_profile-admin-btns-row sticky-top '\n                     style={{marginTop: '-5.75vh'}}>\n\n                    {/*  HOME  */}\n                    <LinkContainer\n                        className='d-inline global_whitehovergoldenrodtxt global_goldenrod-border-left-hover ' to='/'>\n                        <Nav.Link>\n                            <i className='far fa-home'\n                               style={{fontSize: '1.5em'}}> </i>\n                        </Nav.Link>\n                    </LinkContainer>\n\n                    {/*  BROWSE PRODUCTS  */}\n                    <LinkContainer\n                        className='d-inline global_whitehovergoldenrodtxt global_goldenrod-border-left-hover '\n                        to='/products'>\n                        <Nav.Link className=''>\n                            PRODUCTS\n                        </Nav.Link>\n                    </LinkContainer>\n\n                    {/*  AVIONICS  */}\n                    <LinkContainer\n                        className='d-inline global_whitehovergoldenrodtxt global_goldenrod-border-left-hover '\n                        to='/avionics'>\n                        <Nav.Link>\n                            AVIONICS\n                        </Nav.Link>\n                    </LinkContainer>\n\n                    {/*  MAINTENANCE  */}\n                    <LinkContainer\n                        className='d-inline global_whitehovergoldenrodtxt global_goldenrod-border-left-hover '\n                        to='/maintenance'>\n                        <Nav.Link>\n                            MAINTENANCE\n                        </Nav.Link>\n                    </LinkContainer>\n\n                    {/*  EO/IR FLIR  */}\n                    <LinkContainer\n                        className='d-inline global_whitehovergoldenrodtxt global_goldenrod-border-left-hover '\n                        to='/flir'>\n                        <Nav.Link>\n                            FLIR\n\n                        </Nav.Link>\n                    </LinkContainer>\n\n                    {/*  TEAM  */}\n                    <LinkContainer\n                        className='d-inline global_whitehovergoldenrodtxt global_goldenrod-border-left-hover '\n                        to='/team'>\n                        <Nav.Link>\n                            TEAM\n                        </Nav.Link>\n                    </LinkContainer>\n\n                    {/*  WISHLIST  */}\n                    <LinkContainer\n                        className='d-inline global_whitehovergoldenrodtxt global_goldenrod-border-left-hover '\n                        to='/wishlist'>\n                        <Nav.Link className='' style={{height: '6vh'}}>\n                            <>\n                                        <span className={\n                                            cartItems.length > 0 ?\n                                                ' global_goldenrod'\n                                                :\n                                                'global_white'}>\n                                        {cartItems.length}\n\n                                        </span>\n                                &nbsp;\n                                <i className='fal fa-envelope-open-text HeaderDesktop_nav-tab-icon-size'> </i>\n                            </>\n                        </Nav.Link>\n                    </LinkContainer>\n\n                </Col>\n\n\n                {/*============= WISHLIST / LOGIN / CONTACT BTNS ==============*/}\n                <Col xs={2} className='d-flex justify-content-end' style={{marginTop: '-5vh'}}>\n                    <LinkContainer to='/contact' className=''>\n                        <Nav.Link className=''>\n                            <h6 className='global_goldenrod-to-white pl-3'>Contact</h6>\n                        </Nav.Link>\n                    </LinkContainer>\n                </Col>\n\n            </Row>\n        </>\n\n    )\n\n};\n\nexport default HeaderDesktop;","/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/HeaderFooter/Footer.js",[],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/HeaderFooter/HeaderMobile.js",["551","552"],"import React, {useEffect} from 'react