me} to='/'>\n                            <Nav.Link className='flip-container'>\n                                <div className='flipper'>\n                            <span className='SocialIcons_socialIcons front'>\n                                <i className=\"m-auto fab fa-instagram \"> </i>\n                            </span>\n                                    <span className='SocialIcons_socialIcons-size back'>\n                                <i className=\"m-auto fab fa-instagram \"> </i>\n                            </span>\n                                </div>\n                            </Nav.Link>\n                        </LinkContainer>\n                    </Col>\n                    <Col xs={4}>\n                        {/*  TWITTER */}\n                        <LinkContainer className={twitterClassName} to='/'>\n                            <Nav.Link className='flip-container'>\n                                <div className='flipper'>\n                            <span className='SocialIcons_socialIcons front'>\n                                <i className=\"m-auto fab fa-twitter \"> </i>\n                            </span>\n                                    <span className='SocialIcons_socialIcons-size back'>\n                                <i className=\"m-auto fab fa-twitter \"> </i>\n                            </span>\n                                </div>\n                            </Nav.Link>\n                        </LinkContainer>\n                    </Col>\n                    {/*<WishListTab/>*/}\n                </Row>\n            </Nav>\n        </>\n    )\n};\n\nexport default SocialIcons","/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/SearchBox.js",["461","462","463"],"import React, {useState} from 'react'\nimport {Form, Button, Nav} from 'react-bootstrap'\nimport SocialIcons from \"./SocialIcons\";\nimport logoSearch from '../images/graphics/search-logo-small.png'\n\nconst SearchBox = ({history, searchClasses, formClasses, btnClasses, btnSize, searchSize, btnStyle, iconClass, imgClasses}) => {\n    const [keyword, setKeyword] = useState('');\n\n    const submitHandler = (e) => {\n        e.preventDefault();\n        if (keyword.trim()) {\n            history.push(`/search/${keyword}`);\n        } else {\n            history.push('/')\n        }\n        setKeyword('')\n    };\n\n    return (\n        <>\n            <Form onSubmit={submitHandler} inline className={formClasses}>\n                <Form.Control\n                    type='text'\n                    name='searchBar'\n                    value={keyword}\n                    placeholder='search...'\n                    onChange={(e) => setKeyword(e.target.value)}\n                    size={searchSize}\n                    className={searchClasses}\n                >\n                </Form.Control>\n                <Button type='submit' id='StickyHeader_SearchBox_searchImage' size={btnSize} className={btnClasses} style={btnStyle}\n                >\n                    <i className={iconClass}> </i>\n                </Button>\n            </Form>\n        </>\n    )\n};\n\nexport default SearchBox","/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/MulitCarouselPage.js",[],["464","465"],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/JumboCardImgAndText.js",["466","467","468","469","470"],"import React from 'react';\nimport {Row, Col, Container} from \"react-bootstrap\";\nimport panel from \"../images/avionics/panel-divider.jpeg\";\nimport tayseerAvionics from \"../images/avionics/tayseer-1.jpg\";\nimport cncMachine from \"../images/avionics/cnc-machine-divider.png\";\nimport {Jumbotron} from \"reactstrap\";\n\n\nconst JumboCardImgAndText = ({jumboClass, jumboStyle,rowClassName, rowStyle, lgRow, mdRow, xsRow, lgImgCol, mdImgCol, xsImgCol, imgSrc, imgClassName,lgTextCol, mdTextCol, xsTextCol,lgTitleCol, mdTitleCol, xsTitleCol, lgIconTextCol, mdIconTextCol, xsIconTextCol, iconSymbol, lgIconCol, mdIconCol, xsIconCol, lgSubtextCol, mdSubtextCol, xsSubtextCol, lgBlockTextCol, mdBlockTextCol, xsBlockTextCol, title, subText, paragraph}) => {\n    return (\n        // <Jumbotron className={jumboClass} style={jumboStyle} fluid>\n            <Row className={rowClassName} style={rowStyle} lg={lgRow} md={mdRow} xs={xsRow}>\n                <Col lg={lgImgCol} md={mdImgCol} xs={xsImgCol} className='animations_img-hover-zoom animations_img-hover-zoom--slowmo'>\n                    <img src={imgSrc} alt=\"\" className={imgClassName} />\n                </Col>\n                <Col lg={lgTextCol} md={mdTextCol} xs={xsTextCol} >\n                    <Row>\n                        <Col lg={lgTitleCol} md={mdTitleCol} xs={xsTitleCol}>\n                            <h1>{title}</h1>\n                        </Col>\n                        <Col lg={lgIconTextCol} md={mdIconTextCol} xs={xsIconTextCol}>\n                            <Row>\n                                <Col lg={lgIconCol} md={mdIconCol} xs={xsIconCol}>\n                                    <i className={iconSymbol} style={{fontSize: '1.8em'}}> </i>\n                                </Col>\n                                <Col lg={lgSubtextCol} md={mdSubtextCol} xs={xsSubtextCol}>\n                                    <p className='text-muted mt-1 accentFont'>{subText}</p>\n\n                                </Col>\n                            </Row>\n                        </Col>\n                        <Col className='bg-white' lg={lgBlockTextCol} md={mdBlockTextCol} xs={xsBlockTextCol}>\n                            <p>{paragraph}</p>\n                        </Col>\n                    </Row>\n                </Col>\n            </Row>\n        // </Jumbotron>\n    );\n};\n\nexport default JumboCardImgAndText;\n",["471","472"],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/JumboCardImgs.js",["473"],"import React from 'react';\nimport {Row, Col} from \"react-bootstrap\";\nimport '../style/1/Avionics.css'\nimport Zoom from 'react-reveal/Zoom';\n\n\nconst JumboCardImgs = ({imgSrc1, imgSrc2, imgSrc3, imgSrc4, imgSrc5}) => {\n    return (\n        <>\n\n                <Col xs={12} className='d-flex justify-content-center mb-3'>\n                    <Zoom duration={1000}>\n                        <img src={imgSrc1} alt=\"\" className='Avionics_img-border rounded h-50 w-50'/>\n                    </Zoom>\n                </Col>\n                <Col xs={12} className='d-flex justify-content-center mb-3'>\n                    <Zoom duration={1500}>\n                        <img src={imgSrc2} alt=\"\" className='Avionics_img-border rounded h-50 w-50'/>\n                    </Zoom>\n                </Col>\n                <Col xs={12} className='d-flex justify-content-center mb-3'>\n                    <Zoom duration={2000}>\n                        <img src={imgSrc3} alt=\"\" className='Avionics_img-border rounded h-50 w-50'/>\n                    </Zoom>\n                </Col>\n                <Col xs={12}\n                     className='d-flex justify-content-center mb-3'>\n                    <Zoom duration={2500}>\n                        <img src={imgSrc4} alt=\"\" className=' Avionics_img-border rounded h-50 w-50'/>\n                    </Zoom>\n                </Col>\n            <Col xs={12}\n                 className='d-flex justify-content-center mb-3'>\n                <Zoom duration={3000}>\n                    <img src={imgSrc5} alt=\"\" className='Avionics_img-border rounded h-50 w-50'/>\n                </Zoom>\n            </Col>\n\n        </>\n    );\n};\n\nexport default JumboCardImgs;",["474","475"],"/Users/shelby/IdeaProjects/bison-aviation/frontend/src/components/TestimonialCarousel.js",["476"],"import React, {Component} from \"react\";\nimport \"react-responsive-carousel/lib/styles/carousel.min.css\";\nimport {Carousel} from \"react-responsive-carousel\";\nimport '../style/TestimonialCarousel.css'\nimport {Jumbotron} from \"reactstrap\";\nimport Zoom from 'react-reveal/Zoom';\n\nconst TestimonialCarousel = () => {\n    return (\n        <>\n            <Jumbotron className=' global_blood-red-bg mb-0 p-0'\n                       fluid>\n                <Zoom>\n                    <Carousel\n                        showArrows={true}\n                        infiniteLoop={true}\n                        showThumbs={false}\n                        showStatus={false}\n                        autoPlay={true}\n                        interval={3000}\n                    >\n                        <div>\n                            {/*<img src=\"/images/shirley.png\" />*/}\n                            <div className=\"myCarousel global_blood-red-bg\">\n                                <h3 className='global_cultured'>Richard F.</h3>\n                                <h1>\n                                    {/*<i className='fas fa-star global_blood-red'> </i>*/}\n                                    {/*<i className='fas fa-star global_blood-red'> </i>*/}\n                                    {/*<i className='fas fa-star global_blood-red'> </i>*/}\n                                    {/*<i className='fas fa-star  global_blood-red'> </i>*/}\n                                    {/*<i className='fas fa-star global_blood-red'> </i>*/}\n                                    <i className='fas fa-star global_cultured'> </i>\n                                    <i className='fas fa-star global_cultured'> </i>\n                                    <i className='fas fa-star global_cultured'> </i>\n                                    <i className='fas fa-star  global_cultured'> </i>\n                                    <i className='fas fa-star global_cultured'> </i>\n                                </h1>\n                                <p className='global_cultured'>\n                                    Had the chance to work with John from Bison on his V-tail recently and the guy's\n          